 {
    "extends": "default",
    "rules": {
      "avoid-throw": false,
      "avoid-suicide": "error",
      "avoid-sha3": "warn",
      "compiler-fixed": false,
      "indent": ["warn", 2],
      "max-line-length": 150,
      "var-name-mixedcase": false,
      "func-param-name-mixedcase": false
    }
  }